{
    "name": "thantalas/slim-nne",
    "description": "Ninety Nine Enemies Project: Slim Framework in MVC environment and CMS",
    "keywords": ["framework", "slim", "mvc",  "NNe", "Ninety Nine Enemies Project"],
    "license": "MIT",
    "version": "1.0.0",	
    "authors": [
        {
            "name": "Tonelli Giorgio",
            "email": "th.thantalas@gmail.com",
            "homepage":"http://thnet.komunikando.org",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thantalas/slim-nne"
        }
    ],
    "require": {
        "php": ">= 5.3.0",
        "slim/slim": "2.6.*",
        "slim/views":"0.1.2",
        "illuminate/database" : "4.*",
        "ircmaxell/password-compat": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "" : [
		"/Nne",
                "/Nne/Nne",
		"/Nne/Nne/Controllers",
                "app/Controllers/",
		"app/Components/",
		"app/Helpers/",
                "app/Models/",
                "app/Middlewares/",
                "app/Modules/"
            ]
        },
        "psr-0": {
       		"Nne": "Nne/"
  	}
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
